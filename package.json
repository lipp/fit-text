{
  "name": "fit-text-webcomponent",
  "version": "1.0.2",
  "description": "A minimal fit-text web-component / custom-element",
  "keywords": [
    "fit",
    "web component",
    "custom element",
    "text",
    "responsive",
    "width"
  ],
  "main": "dist/index.min.js",
  "scripts": {
    "test": "jest",
    "minify": "uglifyjs --compress --mangle --output dist/index.min.js -- index.js",
    "build": "npm test",
    "prettier": "prettier --no-semi --single-quote --write index.js __tests__/index.js && js-beautify -r examples/*.html",
    "start": "http-server . -s -g"
  },
  "repository": "github:lipp/fit-text",
  "author": "Gerhard Preuss",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "js-beautify": "^1.8.6",
    "prettier": "^1.14.3",
    "puppeteer": "^1.9.0",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {}
}
